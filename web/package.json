  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@primer/stylelint-config": "^13.4.0",
    "esbuild": "^0.25.11",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.38.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "style-loader": "^4.0.0",
    "stylelint": "^16.25.0",
    "stylelint-config-sass-guidelines": "^12.1.0"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
